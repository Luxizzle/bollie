  "module": "dist/bollie.esm.js",
  "size-limit": [
    {
      "path": "dist/bollie.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/bollie.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.4",
    "@types/debug": "^4.1.7",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/node": "^16.10.1",
    "husky": "^7.0.2",
    "size-limit": "^5.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "isomorphic-fetch": "^3.0.0",
    "type-fest": "^0.21.3"
  }
